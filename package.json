        "eslint-plugin-prettier": "^2.6.2",
        "eslint-plugin-react": "^7.10.0",
        "node-sass": "^4.9.2",
        "parcel-bundler": "^1.10.1",
        "parcel-plugin-inlinesvg": "0.0.14",
