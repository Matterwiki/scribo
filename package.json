{
    "name": "scribo",
    "version": "1.0.0",
    "description": "The editor developed for Matterwiki.",
    "main": "index.js",
    "scripts": {
        "prepare": "echo 'Installing peer dependencies..âŒ›' && install-self-peers --npm -- --no-save",
        "start": "echo 'Running md editor example...\n' && npm run example:md",
        "example:md": "parcel examples/md/index.html --no-cache --no-autoinstall",
        "example:wysiwyg": "parcel examples/wysiwyg/index.html --no-cache --no-autoinstall",
        "format": "prettier --write \"{lib,example}/**/*.{js,css}\"",
        "lint": "eslint \"{lib,example}/**/*.{js,css}\""
    },
    "keywords": [
        "matterwiki",
        "editor",
        "wysiwyg",
        "markdown"
    ],
    "peerDependencies": {
        "react": "^16.6.x",
        "react-dom": "^16.6.x"
    },
    "author": "Nishant Arora (@nshntarora) & Krishna Gopinath (@krishwader)",
    "repository": {},
    "license": "ISC",
    "devDependencies": {
        "@team-griffin/install-self-peers": "^1.1.1",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.6",
        "babel-plugin-styled-components": "^1.7.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-3": "^6.24.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-config-prettier": "^2.9.0",
